	year = {2013},
	doi = {10.1007/978-1-4419-5546-3_6},
	pages = {79--94},
	file = {Schwartz et al. - 2013 - Metacognition A Closed-Loop Model of Biased Compe.pdf:C\:\\Users\\s1932788\\Zotero\\storage\\379VEZKI\\Schwartz et al. - 2013 - Metacognition A Closed-Loop Model of Biased Compe.pdf:application/pdf},
