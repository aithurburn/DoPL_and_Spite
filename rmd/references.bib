@incollection{keltner2010,
	address = {New York,  NY,  US},
	title = {Paradoxes of power: {Dynamics} of the acquisition, experience, and social regulation of social power.},
	isbn = {978-1-60623-619-2 (Hardcover)},
	abstract = {Although a great deal of research points to corruption and malevolent facets of power, instances of prosocial behavior have also been documented. In Chapter 6, "Paradoxes of Power: Dynamics of the Acquisition, Experience, and Social Regulation of Social Power," Keltner, Gruenfeld, Galinsky, and Kraus deal with these inconsistencies, pointing out paradoxical effects of power. With respect to the acquisition of social power, they show that power is afforded to those individuals who engage socially in ways that build bonds that advance the interests of the group. With respect to the experience of power, they review evidence showing that power prioritizes the emotions, goals, and actions of high-power individuals, often leading to socially disruptive objectification of others. Finally, with respect to constraints upon power, they reveal how group-based representations (a fellow group member's reputation), communication (gossip), and self-assessments (an individual's modest sense of power) constrain the actions of those in power according to how they advance group interests. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
