	author = {Gino, Francesca and Schweitzer, Maurice E. and Mead, Nicole L. and Ariely, Dan},
	month = jul,
	year = {2011},
	keywords = {Dishonesty, Ego depletion, Ethical decision making, Impulsive cheating, Moral identity, Self-control, Self-regulatory resources, Unethical behavior},
