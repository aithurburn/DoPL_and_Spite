	volume = {30},
	copyright = {Copyright © 2000 John Wiley \& Sons, Ltd.},
	issn = {1099-0992},
	shorttitle = {Why do superiors attend to negative stereotypic information about their subordinates?},
	url = {http://onlinelibrary.wiley.com/doi/abs/10.1002/1099-0992%28200009/10%2930%3A5%3C651%3A%3AAID-EJSP13%3E3.0.CO%3B2-O},
	doi = {10.1002/1099-0992(200009/10)30:5<651::AID-EJSP13>3.0.CO;2-O},
	abstract = {Power can be defined as control over other people's outcomes. Using this definition, we explored the impact of power on attentional processes involved in impression formation. Because powerful individuals may want to maintain and justify their position, powerful participants should pay particular attention to negative stereotype-consistent information about their subordinates. In contrast, powerless participants should devote their attention to stereotype-inconsistent information in an attempt to increase their control over the social context. Study 1 directly manipulated control by assigning participants to the role of leader or subordinate in a task group. Results showed that, compared to subordinates, leaders devoted more attention to negative stereotypic attributes. Study 2 manipulated the legitimacy of power and replicated the pattern found in Study 1 but only when power was illegitimate. Our findings suggest that the experience of power can be associated with feelings of threat, especially when power is illegitimate, thereby orienting impression-formation processes toward information likely to maintain the existing social structure. We discuss our results in the context of current work on motivated social cognition, social identity, and legitimisation. Copyright © 2000 John Wiley \& Sons, Ltd.},
	language = {en},
	number = {5},
	urldate = {2019-11-20},
	journal = {European Journal of Social Psychology},
	author = {Rodríguez‐Bailón, Rosa and Moya, Miguel and Yzerbyt, Vincent},
	year = {2000},
	pages = {651--671},
	file = {Rodríguez‐Bailón et al_2000_Why do superiors attend to negative stereotypic information about their.pdf:C\:\\Users\\s1932788\\Zotero\\storage\\FZWQHI4A\\Rodríguez‐Bailón et al_2000_Why do superiors attend to negative stereotypic information about their.pdf:application/pdf;Snapshot:C\:\\Users\\s1932788\\Zotero\\storage\\Z55ZL9VS\\10)305651AID-EJSP133.0.html:text/html},
